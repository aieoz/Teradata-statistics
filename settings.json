{
    "database_host": "192.168.0.102",
    "database_user": "dbc",
    "analysis_database": "TDSP_ANALYSIS",
    "default_period": 30,
    "analysis": [
        {
            "name": "Tables usage",
            "description": "Measure total usage (updates, selects) of every table in database",
            "target": "target/tables_usage.json",
            "short": "TU",
            "enabled": true,
            "default_databases": [
                "movies"
            ]
        },
        {
            "name": "Daily usage",
            "description": "Measure average usage (updates, selects) of single table during day",
            "short": "DU",
            "target": "target/daily_usage.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        },
        {
            "name": "Traffic type",
            "description": "Joins/Inserts number",
            "short": "TT",
            "target": "target/traffic_type.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        },
        {
            "name": "Inserts counts for tables",
            "description": "Insert operations",
            "short": "INS",
            "target": "target/inserts.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        }
    ]
}