{
    "database_host": "192.168.0.103",
    "database_user": "dbc",
    "default_period": 30,
    "odbc_location": "/opt/teradata/client/",
    "analysis": [
        {
            "name": "Tables usage",
            "description": "Measure total usage (updates, selects) of tables.",
            "target": "target/tables_usage.json",
            "short": "TU",
            "enabled": true,
            "default_databases": [
                "movies"
            ]
        },
        {
            "name": "Daily usage",
            "description": "Measure average usage (updates, selects) tables during day.",
            "short": "DU",
            "target": "target/daily_usage.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        },
        {
            "name": "Traffic type",
            "description": "Joins/Inserts actions proportions for tables.",
            "short": "TT",
            "target": "target/traffic_type.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        },
        {
            "name": "Inserts counts for tables",
            "description": "Every day insert operations count.",
            "short": "INS",
            "target": "target/inserts.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        },
        {
            "name": "Traffic type by user",
            "description": "Traffic type (select, insert...) generated by different users.",
            "short": "TTU",
            "target": "target/traffic_type_with_user.json",
            "enabled": true,
            "default_tables": [
                "movies.movies"
            ]
        }
    ]
}

